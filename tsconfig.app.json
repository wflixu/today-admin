{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": ["env.d.ts", "src/**/*.ts", "src/**/*.vue", "src/**/*.tsx", "src/**/*.d.ts", "mock/**/*.ts" ],
  "exclude": ["src/**/__tests__/*",],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    },
    "types": ["element-plus/global", "unplugin-vue-define-options/macros-global", "src/types/app.d.ts", "vite/client"]
  }
}
